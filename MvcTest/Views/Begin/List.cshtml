
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<MvcTest.Models.Member>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table>
    <tr>
        <th>氏名</th><th>メールアドレス</th><th>誕生日</th><th>既婚</th><th>備考</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Birth</td>
            <td>@item.Married</td>
            <td>@item.Memo</td>
        </tr>
    }
</table>
